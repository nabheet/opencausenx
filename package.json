{
  "name": "opencausenx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export DATABASE_URL='postgresql://opencausenx:opencausenx_dev@localhost:5432/opencausenx' && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "export DATABASE_URL='postgresql://opencausenx:opencausenx_dev@localhost:5432/opencausenx' && prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "export DATABASE_URL='postgresql://opencausenx:opencausenx_dev@localhost:5432/opencausenx' && tsx prisma/seed.ts",
    "db:studio": "export DATABASE_URL='postgresql://opencausenx:opencausenx_dev@localhost:5432/opencausenx' && prisma studio",
    "setup": "npm install && prisma generate && npm run db:migrate -- --name init && npm run db:seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
